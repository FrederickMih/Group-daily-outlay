 <div class="card blue lighten-5">
<% if false %>
  <div class="card-content teal-text darken-4">
    <%= outlay_icon(@outlay) %>


    <p class="travel_show_text"><span class="brown-text darken-4">This:</span> <%= @outlay.name %></p>
    <p class="travel_show_text"><span class="brown-text darken-4">Was Bought At:</span>$<%= @outlay.amount %></p>
    <p class="travel_show_text"><span class="brown-text darken-4">Date:</span> <%= @travel.created_at.to_s(:long_ordinal) %></p>
  </div>
  <div class="card-action right-align">
    <%= link_to '<i class="small material-icons green-text darken-4">arrow_back</i>'.html_safe, external_path %>
    <%= link_to '<i class="small material-icons">mode_edit</i>'.html_safe, edit_travel_path(@outlay) %>
    <%= link_to '<i class="small material-icons red-text accent-3">delete</i>'.html_safe, @outlay, method: :delete, data: { confirm: 'Are you sure?' } %>
  </div>
  <% end %> 
</div>




